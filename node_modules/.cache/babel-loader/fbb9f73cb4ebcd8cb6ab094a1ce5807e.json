g from \"vue-loading-overlay\"; // Import component\r\nimport \"vue-loading-overlay/dist/vue-loading.css\"; // Import stylesheet\r\nimport App from \"./App.vue\";\r\nimport router from \"./router\";\r\nimport axios from \"axios\";\r\nimport VueAxios from \"vue-axios\";\r\n\r\nimport \"bootstrap\";\r\n\r\n\r\nVue.config.productionTip = false;\r\n// Vue.prototype.$ = $; // 給vue原型上新增 $ \r\n\r\n// 元件 全域註冊\r\nVue.component(\"Loading\", Loading);\r\n// 套件加入到 Vue 的藍圖(原型)內\r\nVue.use(VueAxios, axios);\r\n\r\n// 金額添加貨幣別\r\nVue.filter(\"dollarSign\", (price)=>{\r\n  return `$ ${price}`;\r\n});\r\n// 金額添加千分位\r\nVue.filter(\"currency\", (num)=>{\r\n  // 切分小數與整數\r\n  let _parts = num.toString().split(\".\");\r\n  _parts[0] = _parts[0].replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\r\n  return _parts.join(\".\");\r\n});\r\n\r\nnew Vue({\r\n  router,\r\n  render: h => h(App)\r\n}).$mount(\"#app\");\r\n"]}]}